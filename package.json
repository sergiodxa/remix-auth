{
  "name": "remix-auth",
  "version": "2.1.0",
  "description": "Simple Authentication for Remix",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "scripts": {
    "build:types": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir build --esModuleInterop",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --outfile=build/index.mjs --external:crypto --external:remix",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --outfile=build/index.js --external:remix",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "jest --config=config/jest.config.ts --passWithNoTests",
    "setup": "remix setup",
    "postinstall": "npm run setup"
  },
  "keywords": [
    "remix",
    "auth",
    "authentication",
    "local",
    "auth0",
    "oauth2",
    "strategies"
  ],
  "author": {
    "name": "Sergio Xalambr√≠",
    "email": "hello@sergiodxa.com",
    "url": "https://sergiodxa.com"
  },
  "repository": {
    "url": "https://github.com/sergiodxa/remix-auth",
    "type": "git"
  },
  "homepage": "https://github.com/sergiodxa/remix-auth#readme",
  "license": "MIT",
  "files": [
    "build",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
    "@remix-run/node": "^0.20.1",
    "@remix-run/react": "^0.20.1",
    "remix": "^0.19.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@remix-run/dev": "^0.19.2",
    "@remix-run/node": "^0.19.2",
    "@remix-run/react": "^0.19.2",
    "@types/jest": "^26.0.23",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "esbuild": "^0.12.15",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-testing-library": "^4.3.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.6",
    "remix": "^0.20.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  }
}
